{
  "name": "@wyny/crypto",
  "version": "1.1.5",
  "description": "aes+rsa加解密工具",
  "main": "./dist/crypto.umd.js",
  "module": "./dist/crypto.es.js",
  "exports": {
    ".": {
      "import": "./dist/crypto.es.js",
      "require": "./dist/crypto.umd.js"
    }
  },
  "type": "module",
  "author": "wangqs",
  "license": "ISC",
  "keywords": [
    "typescript",
    "vite",
    "crypto"
  ],
  "scripts": {
    "build": "vite build --mode admin",
    "build:offiaccount": "vite build --mode offiaccount",
    "start": "vite",
    "cp": "mkdir -p viewKit && cp -r ./dist/ ./viewKit/ &&cp -r ./index.html ./viewKit/",
    "preview": "yarn build && yarn cp",
    "test": "yarn build && jest",
    "release": "yarn build && np --no-2fa --branch main --message v%s",
    "release:test": "yarn build && np --no-2fa --branch main --message v%s --no-git "
  },
  "publishConfig": {
    "registry": "http://172.16.11.82:4873/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@peculiar/webcrypto": "^1.3.3",
    "@vitejs/plugin-vue": "^2.3.0",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "np": "^7.6.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vue": "3.2.31"
  }
}
